<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fish.vwhub.mapper.VwHubInputMapper">

    <resultMap id="BaseResultMap" type="com.fish.vwhub.entity.VwHubInput">
            <id property="id" column="id" jdbcType="OTHER"/>
            <result property="fileName" column="file_name" jdbcType="VARCHAR"/>
            <result property="fileType" column="file_type" jdbcType="OTHER"/>
            <result property="gmtCreate" column="gmt_create" jdbcType="TIMESTAMP"/>
            <result property="finishTime" column="finish_time" jdbcType="TIMESTAMP"/>
    </resultMap>


    <resultMap id="MergeResultMap" type="com.fish.vwhub.entity.VwHubInput">
        <id property="id" column="id"/>
        <result property="fileName" column="file_name" jdbcType="VARCHAR"/>
        <result property="fileType" column="file_type" jdbcType="INTEGER"/>
        <result property="gmtCreate" column="gmt_create" jdbcType="TIMESTAMP"/>
        <result property="finishTime" column="finish_time" jdbcType="TIMESTAMP"/>
        <collection property="results" ofType="com.fish.vwhub.entity.VwHubOutput">
            <id property="id" column="out_id" jdbcType="INTEGER"/>
            <result property="inputId" column="input_id" jdbcType="OTHER"/>
            <result property="outputFileName" column="output_file_name" jdbcType="VARCHAR"/>
            <result property="gmtCreate" column="gmt_create" jdbcType="TIMESTAMP"/>
        </collection>
    </resultMap>

    <sql id="Base_Column_List">
        id,file_name,file_type,gmt_create,finish_time
    </sql>
    <select id="manualPage" resultMap="MergeResultMap">
        select  i.id,i.file_name,i.file_type,i.gmt_create,i.finish_time,vho.id out_id,vho.input_id,vho.output_file_name,vho.gmt_create
        from vw_hub_input i left join vw_hub_output vho on i.id = vho.input_id
        order by i.id desc
    </select>
</mapper>
